<?xml version="1.0" encoding="UTF-8"?>

<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<application>
		<resource-bundle>
			<base-name>resources</base-name>
			<var>msgs</var>
		</resource-bundle>
	</application>
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>com.sarp.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>display</managed-bean-name>
		<managed-bean-class>com.sarp.DisplayBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>numero</managed-bean-name>
		<managed-bean-class>com.sarp.NumeroBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>puesto</managed-bean-name>
		<managed-bean-class>com.sarp.PuestoBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>sector</managed-bean-name>
		<managed-bean-class>com.sarp.SectorBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>test</managed-bean-name>
		<managed-bean-class>com.sarp.TestBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>tramite</managed-bean-name>
		<managed-bean-class>com.sarp.TramiteBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
</faces-config>